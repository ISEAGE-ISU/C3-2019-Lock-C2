{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ".",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "/public/out/docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "meta": {
      "title": "Docs",
      "description": "VERY SECURE LOCK",
      "keyword": "no u"
    },
    "search": true,
    "collapse": true,
    "wrap": true,
    "typedefs": true,
    "private": true
  }
}
